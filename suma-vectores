// vectores o arreglos.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main()
{
	int n;
	int numeros[100];
	int suma=0;
	cout<< "ingrese numero de elementos: " ;
	cin>> n;
	for (int i=0; i<n; i++)
	{
		cout<< "ingrese el numero: ";
		cin>> numeros[i];
	}

	for(int i=0; i<n; i++)
	{ 
		suma=suma+numeros[i];
	}
	cout<< " La suma de los elementos del vector es: " <<suma<<endl;

    getch();
}
