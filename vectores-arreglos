// vectores o arreglos.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main()
{
	int numeros[]={1,2,3,4,5};
	int suma=0;
	for(int i=0; i<5; i++)
	{ 
		suma=suma+numeros[i];
	}
	cout<< " La suma de los elementos del vector es: " <<suma<<endl;

    getch();
}
