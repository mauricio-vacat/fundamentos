#include "stdafx.h"
#include <iostream>
#include "conio.h"
using namespace std;
void main ()
{
	float a,b,area;
	cout<< "Ingresar Ancho del Rectangulo: "<<endl;
	cin>>a;
	cout<< "Ingresar Alto del Rectangulo: "<<endl;
	cin>>b;
	area= a*b;
	cout<< "Area del Rectangulo: "<<endl;
	cout<< area;
	getch();
}
   
